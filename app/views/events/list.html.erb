<p id="notice"><%= notice %></p>

<h1>List</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount of Points</th>
      <th>Location</th>
      <th>Date</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.event_point %></td>
        <td><%= event.event_location %></td>
        <td><%= event.event_date %></td>
        <td><%= event.event_start %></td>
        <td><%= event.event_end %></td>
      </tr>
    <% end %>
  </tbody>
</table>
